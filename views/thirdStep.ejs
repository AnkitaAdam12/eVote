    </div>
    <% if (msg == 'User Not Found') { %>
        <div id="myModal" class="modal1" >
          <!-- Modal content -->
          <div class="modal-content">
            <span class="close1" id="close">&times;</span>
            <p class="msg"><%= msg %><br><br>
              <a href="/thirdStep"><input type="button" value="OK" class="ok" id="ok" name="ok"></a></p>
          </div>
        </div>
        <% } else if (msg == 'Face Not Detected!')  {%>
          <div id="myModal" class="modal1" >
            <!-- Modal content -->
            <div class="modal-content">
              <span class="close1" id="close">&times;</span>
              <p class="msg"><%= msg %><br><br>
              <a href="/thirdStep"><input type="button" class="ok" value="OK" id="ok" name="ok"></a></p>
              <!-- <button id="ok" name="ok" onclick="location.href='Partylogin'">OK</button> -->
            </div>
          </div>
          <% } else if (msg == 'User Not Recongnized')  {%>
            <div id="myModal" class="modal1" >
              <!-- Modal content -->
              <div class="modal-content">
                <span class="close1" id="close">&times;</span>
                <p class="msg"><%= msg %><br><br>
                <a href="/thirdStep"><input type="button" class="ok" value="OK" id="ok" name="ok"></a></p>
                <!-- <button id="ok" name="ok" onclick="location.href='Partylogin'">OK</button> -->
              </div>
            </div>
          <% } else {%>
