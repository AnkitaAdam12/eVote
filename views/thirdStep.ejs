    <div class="head-container">
        <div id="shadow">
        <label class="reg">Voting Area</label>
        <form action="/thirdAuth" method="post" id="ts">
            <div class="form first">
                <div class="details personal">
                    <!-- <span class="title">Personal Details</span> -->

