<body>
    <header class="header_section">
        <a class="navbar-brand">
            <span>
            .e-Vote 
            </span>
        </a>
      </header>

     
      <!-- end header section -->
    <div class="head-container">
        <label class="reg">Registration</label>

        <form action="/partyReg" method="post" enctype="multipart/form-data" id="partyreg">
            <div class="form first">
                <div class="details personal">
                    <span class="title">Party Details</span>

                    <div class="fields">

                        <div class="input-field">
                            <label>Party Leader Name</label>
                            <input type="text" placeholder="Enter Leader name"  id="name" name="name">
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                            <label>Leader's Date of Birth</label>
                            <input type="date" placeholder="Enter birth date"  id="dob" name="dob">
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                          <label>Party Name</label>
                          <input type="text" placeholder="Enter party name"  id="party" name="party">
                          <i class="fa-solid fa-circle-check"></i>
                          <i class="fa-solid fa-circle-exclamation"></i>
                          <small class="small">Error message</small>
                      </div>

                        <div class="input-field">
                            <label>Party Member's count</label>
                            <input type="number" placeholder="Enter count of member"  id="count" name="count">
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                            <label>e-Mail</label>
                            <input type="text" placeholder="Enter your e-mail"  id="email" name="email">
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                            <label>Logo of Party (JPG/JPEG/PNG/PDF)</label>
                            <!-- <input type="text" placeholder="Choose your photo" required> -->
                            <input type="file" id="file" accept="image/png, image/jpg, image/jpeg" id="picture" name="picture">
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                            <label>Password</label>
                            
                            <input type="password" placeholder="Create password"  id="pwd1" name="pwd1">
                            <span id="password-icon" class="fa fa-eye"></span>
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                        <div class="input-field">
                            <label>Confirm Password</label>
                            <input type="password" placeholder="Confirm password"  id="pwd2" name="pwd2">
                            <span id="password-icon2" class="fa fa-eye"></span>
                            <i class="fa-solid fa-circle-check"></i>
                            <i class="fa-solid fa-circle-exclamation"></i>
                            <small class="small">Error message</small>
                        </div>

                    </div>
                </div>

                <button class="sumbit" type="button" onclick="submitForm()">
                    <span class="btnText">Submit</span>
                    <i class="uil uil-navigator"></i>
                </button>
            </div>


        </form>
    </div>
    <% if (msg != null) { %>
    <div id="myModal" class="modal1" >
      <!-- Modal content -->
      <div class="modal-content">
        <span class="close1" id="close">&times;</span>
        <p class="msg"><%= msg %><br><br>
          <a href="/Partylogin"><input type="button" class="ok" value="OK" id="ok" name="ok"></a></p>
      </div>
    </div>
    <% } else {%>
      <div id="myModal" class="modal" >
        <!-- Modal content -->
        <div class="modal-content">
          <span class="close" id="close">&times;</span>
          <p class="msg">Record inserted successfully!<br><br>
          <a href="/Partylogin"><input type="button" class="ok" value="OK" id="ok" name="ok"></a></p>
          <!-- <button id="ok" name="ok" onclick="location.href='Partylogin'">OK</button> -->
        </div>
      </div>
      <% } %>
    
